<Window x:Class="PL.EnterOrderTracking"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="EnterOrderTracking" Height="250" Width="300">
    <Grid Margin="10,0,0,0">
        <Button Content="Tracking" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Margin="86,136,0,0" VerticalAlignment="Top" Click="btnTracking_Click" FontSize="18" Height="41" Width="124">
        </Button>
        <Label Content="Order Number" HorizontalAlignment="Left" Margin="33,68,0,0" VerticalAlignment="Top" Width="101" Height="33" />
        <TextBox x:Name="txtInsertId" HorizontalAlignment="Left" Margin="144,71,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101" RenderTransformOrigin="0.5,0.5" Height="27"/>
    </Grid>
</Window>
